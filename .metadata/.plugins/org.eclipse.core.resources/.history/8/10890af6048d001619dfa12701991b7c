class Teste():

    def multiplicar_range_por_dois(self, numero):
        for p in range(1, numero + 1):
            return p * 2
            
    def escrever_no_console(self, frase):
        print frase
        
    def testar_resto_igual_zero(self, numero, divisor):
        for i in numero:
            if i % divisor == 0:
                print(i, '/3 = ', i / divisor)
            else:
                print(i, 'nao resta zero na divisao')
