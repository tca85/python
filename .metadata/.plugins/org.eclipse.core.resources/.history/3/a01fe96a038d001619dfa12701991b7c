class Teste():

    def multiplicar_range_por_dois(self, numero):
        for p in range(1, numero + 1):
            self.escrever_no_console(p * 2)
            
    def escrever_no_console(self, frase):
        print frase
        
    def testar_dividendo_3_zero(self, numero):
        for i in numero:
            if i % 3 == 0:
                print(i, '/3 = ', i / 3)
                #self.escrever_no_console(i, '/3 = ', i / 3)
