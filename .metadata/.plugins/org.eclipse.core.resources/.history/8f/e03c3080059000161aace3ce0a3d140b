class Cadastro():
    #-----------------------------------------------------------------------------------------
    def nome(self, nomes):
        print 'Digite o nome:'
        nome = raw_input()
        
        nomes.append(nome)

    #-----------------------------------------------------------------------------------------
    def verificarIdade(self):
        # raw_input recebe uma string, por isso precisa converter
        idade = raw_input()
        
        return int(idade)
        
    #-----------------------------------------------------------------------------------------